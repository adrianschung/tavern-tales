<div class="container col-12 col-lg-6">
  <h3>Edit Map</h3>
  <%= form_for @map, url: { action: "update" }, html: { multipart: true } do |form| %>
    <%= render partial: 'form', locals: { f: form, action: "edit" } %>
  <% end %>
  <%= link_to 'Delete Map', map_path(current_map), class: "btn btn-danger", method: :delete %>
</div>