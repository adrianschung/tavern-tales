<br />
<div class="container col-12 col-lg-6">
  <div class="row">
    <h3 class="col-6 col-lg-10">Maps</h3>
      <div class="col-6 col-lg-2">
        <%= link_to 'New map', new_map_path, class:'btn btn-secondary float-right' %>
      </div>
  </div>
  <div class="digg_pagination">
    <%= will_paginate @maps %>
  </div>
  <% @maps.each do |map| %>
    <%= render partial: 'map', locals: { map: map } %>
  <% end %>
  <div class="digg_pagination">
    <%= will_paginate @maps %>
  </div>
</div>