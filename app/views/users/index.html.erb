<div class="container col-12 col-lg-6">
  <h1>Users</h1>
  <div class="digg_pagination">
    <%= will_paginate @users %>
  </div>
  <% @users.each do |user| %>
    <div class="user-box">
      <h3><%= link_to user.username, user_path(user) %></h3>
    </div>
  <% end %>
  <div class="digg_pagination">
    <%= will_paginate @users %>
  </div>
</div>